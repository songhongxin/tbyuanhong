<?xml version="1.0" ?>
<SpoutConfig>
	<LogGenerator class="com.etao.lz.star.spout.FileLogGenerator" name="p4p_pv2">
		<Configuration>
			<Attribute name="input_path" value="/local_p4p/p4p.data" />
			<Attribute name="log_builder" value="com.etao.lz.star.spout.FieldsLogBuilder"/>
			<Attribute name="log_class" value="flow" />
			<Attribute name="seperators" value="1,2,3" />
			<Attribute name="tt_tag" value="p4p_pv2" />
			<Attribute name="emit_check_period" value = "0"/>
		</Configuration>
		<LogMapping name="p4p_pv2">
			<Mapping name="log_version" value="@int 2"/>
			<Mapping name="log_src" value = "p4p_pv2"/>
			<Mapping name="dest_type" value = "p4p_pv2"/>
			<Mapping name="ts" value = "@mul 1000 @bigint @field2 2 1"/>
			<Mapping name="customer_id" value = "@field3 7 0 3"/>
		</LogMapping>
	</LogGenerator>	
</SpoutConfig>
