<?xml version="1.0" ?>
<SpoutConfig>
	<LogGenerator class="com.etao.lz.star.spout.FileLogGenerator" name="KGB_click">
		<Configuration>
			<Attribute name="input_path" value="/local_click/click.data" />
			<Attribute name="log_builder" value="com.etao.lz.star.spout.FieldsLogBuilder"/>
			<Attribute name="log_class" value="flow" />
			<Attribute name="seperators" value="1,2,3" />
			<Attribute name="emit_check_period" value = "0"/>
			<Attribute name="tt_tag" value="KGB_click" />
		</Configuration>
		<LogMapping name="KGB_click">
			<Mapping name="log_version" value="@int 2"/>
			<Mapping name="log_src" value = "KGB_click"/>
			<Mapping name="dest_type" value = "KGB_click"/>
			<Mapping name="ts" value = "@mul 1000 @bigint @field2 2 4"/>
			<Mapping name="customer_id" value = "@field2 2 13"/>
			<Mapping name="click_price" value = "@bigint @field2 2 14"/>
		</LogMapping>
	</LogGenerator>	
</SpoutConfig>
