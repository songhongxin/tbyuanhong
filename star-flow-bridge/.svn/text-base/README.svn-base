# star_storm
star-storm.jar
	storm jar star-storm.jar com.etao.lz.star.StarMain flow star-flow
	storm jar star-storm.jar com.etao.lz.star.StarMain business star-business

tools/monitor-http.jar:
	java -jar monitor-http.jar
	lynx localhost:9999

tools/hbase-table.jar
	HBaseTable -create table-name
	HBaseTable -delete table-name
	HBaseTable -scan table-name [-logtype flow|business] [-output path] [-timerange seconds]

tables:
	browse_log,ad_ex_click,biz_order,pay_order,alipay_rtn_order,biz_order_index,pay_order_index

subscriber.jar:
	java -cp lib/protobuf-java-2.4.1.jar:tools/subscriber.jar:lib/log4j-1.2.16.jar:lib/commons-logging-1.1.1.jar:lib/curator-framework-1.0.1.jar:lib/curator-client-1.0.1.jar:lib/guava-10.0.1.jar:lib/zookeeper-3.3.3.jar:lib/slf4j-log4j12-1.5.8.jar:lib/slf4j-api-1.5.8.jar:lib/jzmq-2.1.0.jar com.etao.lz.star.StarLogSubscriber 172.24.208.124:3456

